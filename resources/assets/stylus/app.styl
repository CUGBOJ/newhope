[v-cloak]
    display none

.layout
    border 1px solid #d7dde4
    background #f5f7f9
    position relative
    border-radius 4px
    overflow hidden

.layout-logo
    width 100px
    height 30px
    background #5b6270
    border-radius 3px
    float left
    position relative
    top 15px
    left 20px

.layout-nav
    width 60%
    margin 0 auto
    margin-right 20px

.layout-footer-center
    text-align center
